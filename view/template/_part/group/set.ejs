<!-- _part/group/set -->
<%
let params = {
    active: typeof active !== 'undefined' && active ? 'active' : '',
    attraction: typeof attraction === 'undefined' ? '' : JSON.stringify(attraction),
    translate: typeof translate === 'undefined' ? '' : translate
};
%>
<fieldset class="form-base-group form-set <%- params.active %>" data-attraction='<%- params.attraction %>'>
    <legend><span class="form-set-toggle"><span class="fa fa-plus"></span>
        <span data-t="<%- params.translate %>"><%- title %></span></span>
    </legend>
    <div class="form-set-body form-base-group-body">
    <% for (let item of items) { %>
        <% if (item.template) { %>
        <%- include(item.template, item.params) %>
        <% } else { %>
        <%- item %>
    <% }} %>
    </div>
</fieldset>