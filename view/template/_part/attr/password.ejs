<!-- _part/attr/password -->

<% const p = {}; %>
<%- include('_params', {p}) %>

<div class="form-attr form-group<%- p.required %>" data-handler="<%= p.handler %>">
    <label for="<%- p.id %>" class="control-label <%- p.labelCss %>"><%- p.label %></label>
    <div class="<%- p.valueCss %>">
        <input id="<%- p.id %>" name="<%- p.name %>" type="password" class="form-control"/>
        <div class="hint-block"><%- p.hint %></div>
        <div class="error-block"></div>
    </div>
</div>
